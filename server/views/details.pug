extends layout

block content
  h1.titolo= title
  p.paragrafo #{title} 
  p.paragrafo This is #{nickname} page
  div.paragrafo.dettagli 
    h2 Personal sheet
    p Full name: #{fullname}
    p Age: #{age}
    p Interests:
    ul
      each item in interests
        li #{item} 
    a(href=`${link}`, class="link") Back to home page

